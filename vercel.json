{
  "version": 2,
  "builds": [{ "src": "next.config.js", "use": "@vercel/next" }],
  "buildCommand": "next build",
  "outputDirectory": ".next",
  "framework": "nextjs",
  "rewrites": [
    { "source": "/(.*)", "destination": "/" }
  ],
  "routes": [
    { "src": "/api/(.*)", "dest": "/api/$1.js" }
  ],
  "env": {
    "NEXT_TELEMETRY_DISABLED": "1"
  }
}
